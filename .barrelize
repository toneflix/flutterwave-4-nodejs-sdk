{
  "$schema": "node_modules/barrelize/schema.json",
  "semi": false,
  "barrels": [
    {
      "root": "src",
      "name": "index.ts",
      "order": [
        "utilities"
      ],
      "include": [
        "**/*.ts"
      ],
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts",
        "**/Contracts/**",
        "utilities/global.ts"
      ]
    },
    {
      "root": "src/Contracts",
      "name": "index.ts",
      "include": [
        "**/*.ts"
      ],
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    }
  ]
}